<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProtectedResource</servlet-name>
        <servlet-class>ProtectedResource</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginHandler</servlet-name>
        <servlet-class>LoginHandler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HttpsSampleServlet</servlet-name>
        <servlet-class>HttpsSampleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>passprototype</servlet-name>
        <servlet-class>passprototype</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>passprototype2</servlet-name>
        <servlet-class>passprototype2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProtectedResource</servlet-name>
        <url-pattern>/ProtectedResource</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginHandler</servlet-name>
        <url-pattern>/LoginHandler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HttpsSampleServlet</servlet-name>
        <url-pattern>/HttpsSampleServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>passprototype</servlet-name>
        <url-pattern>/passprototype</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>passprototype2</servlet-name>
        <url-pattern>/passprototype2</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <login-config>
        <auth-method>CLIENT-CERT</auth-method>
        <realm-name>nitin</realm-name>
    </login-config>
    <security-constraint>
        <web-resource-collection>
            <web-resource-name>Entire Application</web-resource-name>
            <url-pattern>/*</url-pattern>
        </web-resource-collection>
        <user-data-constraint>
            <transport-guarantee>CONFIDENTIAL</transport-guarantee>
        </user-data-constraint>
    </security-constraint>
</web-app>
